 pipeline {
  agent any

  stages {
   
    stage('Build') {
      steps {
        // Run the maven build
        sh '"mvn" -Dmaven.test.failure.ignore clean install'
      }

    }
    stage('Deploy') {
      steps {
          deploy adapters: [tomcat9(credentialsId: 'tomcat', path: '', url: 'http://172.31.8.232:8080')], contextPath: 'myapp', war: '**/*.war'
      }
    }
  }
}
